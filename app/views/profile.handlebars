<section class="section">
  <div class="container is center">
    <h1 class="title">
      Welcome to artGarden {{firstName}} {{lastName}}!
    </h1>
    <figure class="image is-128x128">
      {{!-- test   --}}
      <img src="/userdata/profile/{{Userdetail.dataValues.profileImg}}" alt="Image">
    </figure>
    <p class="subtitle">
      <strong>search gigs YOUR PASSWORD IS {{password}}</strong>!

    </p>

    <h2>Requests not completed!</h2>

    <ul>
      {{#each Requests}}
      {{#unless complete}}
      <li>{{dataValues.description}}</li>
      {{!-- {{> request/request-block}} --}}
      {{/unless}}
      {{/each}}
    </ul>

    <h2>Requests completed!</h2>

    <ul>
      {{#each Requests}}
      {{#if complete}}
      <li>{{dataValues}}</li>
      {{!-- {{> request/request-block}} --}}
      {{/if}}
      {{/each}}
    </ul>


    <!-- insert box and media object here -->

    <div class="box">
      <article class="media">
        <div class="media-left">
          <figure class="image is-64x64">
            {{!-- test   --}}
            <img src="/userdata/profile/male-blue-1.png" alt="Image">
          </figure>
        </div>
        <div class="media-content">
          <div class="content">
            <p>
              {{!-- insert username --}}
              <strong>firstName is {{firstName}}</strong> <small>@{{email}}</small> <small>31m</small>
              <br>
              Is verified? {{is_verified}}
              Lorem ipsum dolor sit amet, consectetur adipiscing elit. Aenean efficitur sit amet massa fringilla
              egestas. Nullam condimentum luctus turpis.
            </p>
          </div>
          <nav class="level is-mobile">
            <div class="level-left">
              <a class="level-item" aria-label="reply">
                <span class="icon is-small">
                  <i class="fas fa-reply" aria-hidden="true"></i>
                </span>
              </a>
              <a class="level-item" aria-label="retweet">
                <span class="icon is-small">
                  <i class="fas fa-retweet" aria-hidden="true"></i>
                </span>
              </a>
